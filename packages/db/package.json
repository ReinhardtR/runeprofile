{
  "name": "@runeprofile/db",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "db:generate": "drizzle-kit generate --config=drizzle.config.ts",
    "db:apply:remote": "drizzle-kit migrate --config=drizzle.config.ts",
    "dev:setup": "pnpm db:apply && pnpm db:seed"
  },
  "main": "./src/index.ts",
  "module": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "require": "./src/index.ts"
    }
  },
  "dependencies": {
    "@runeprofile/runescape": "workspace:*",
    "drizzle-orm": "^0.45.1",
    "postgres": "^3.4.7"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250214.0",
    "@types/node": "^20.19.11",
    "dotenv": "^17.2.3",
    "drizzle-kit": "^0.31.8",
    "tsx": "^4.19.2",
    "typescript": "^5.8.2",
    "wrangler": "^4.10.0"
  }
}
