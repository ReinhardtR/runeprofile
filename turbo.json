{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "typecheck": {
      "dependsOn": ["^typecheck"]
    },
    "dev:api-client": {
      "persistent": false,
      "cache": false
    },
    "dev": {
      "persistent": true,
      "cache": false
    }
  }
}
